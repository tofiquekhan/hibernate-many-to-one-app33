<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    <class name="myproject.manytoone.entity.Student" table="std1">
    <id name="sid" length="10"/>
    <property name="sname" length="10"/>
    <property name="saddr" length="10"/>
    <many-to-one name="branch" class="myproject.manytoone.entity.Branch" cascade="all"/>
    </class>
    
    
    
    
    </hibernate-mapping>